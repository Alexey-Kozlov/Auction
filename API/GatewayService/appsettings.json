{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "Yarp": "Debug"
      },
      "FormatterName": "Systemd",
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "dd.mm.yyyy HH:mm:ss > ",
        "UseUtcTimestamp": false
      }
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "Secret": "MySecret 1234567890 и еще что-то там для длинного ключа"
  },
  "ClientApp": "http://localhost:3000",
  "CacheImageExpirationDays": 1,
  "ReverseProxy": {
    "Routes": {
      "auctionsRead": {
        "ClusterId": "auctions",
        "Match": {
          "Path": "/api/auctions/{**catch-all}",
          "Methods": [
            "GET"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/auctions/{**catch-all}"
          }
        ]
      },
      "auctionsWrite": {
        "ClusterId": "auctions",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/auctions/{**catch-all}",
          "Methods": [
            "POST",
            "PUT",
            "DELETE"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/auctions/{**catch-all}"
          }
        ]
      },
      "search": {
        "ClusterId": "search",
        "Match": {
          "Path": "/api/search/{**catch-all}",
          "Methods": [
            "GET",
            "POST"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/search/{**catch-all}"
          }
        ]
      },
      "identity": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/api/identity/{**catch-all}",
          "Methods": [
            "POST"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/auth/{**catch-all}"
          }
        ]
      },
      "bids": {
        "ClusterId": "bids",
        "Match": {
          "Path": "/api/bids/{**catch-all}",
          "Methods": [
            "GET"
          ]
        }
      },
      "notifications": {
        "ClusterId": "notifications",
        "CorsPolicy": "customPolicy",
        "Match": {
          "Path": "/api/notifications/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "notifications/{**catch-all}"
          }
        ]
      },
      "finance": {
        "ClusterId": "finance",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/finance/{**catch-all}",
          "Methods": [
            "GET",
            "POST"
          ]
        }
      },
      "processing": {
        "ClusterId": "processing",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/processing/{**catch-all}",
          "Methods": [
            "GET",
            "POST"
          ]
        }
      }
    },
    "Clusters": {
      "auctions": {
        "Destinations": {
          "auctionApi": {
            "Address": "http://auction-auction-clusterip"
          }
        }
      },
      "search": {
        "Destinations": {
          "searchApi": {
            "Address": "http://auction-search-clusterip"
          }
        }
      },
      "identity": {
        "Destinations": {
          "identityApi": {
            "Address": "http://auction-identity-clusterip"
          }
        }
      },
      "bids": {
        "Destinations": {
          "bidApi": {
            "Address": "http://auction-bid-clusterip"
          }
        }
      },
      "notifications": {
        "Destinations": {
          "notifyApi": {
            "Address": "http://auction-notify-clusterip"
          }
        }
      },
      "finance": {
        "Destinations": {
          "financeApi": {
            "Address": "http://auction-finance-clusterip"
          }
        }
      },
      "processing": {
        "Destinations": {
          "processingApi": {
            "Address": "http://auction-processing-clusterip"
          }
        }
      }
    }
  },
  "MetricGroup": "GateWayMetric",
  "Otlp": {
    "Endpoint": "http://otc-ip-service-import:4317"
  },
  "Redis": {
    "Config": "redis-cluster-ip-service:6379"
  },
  "GrpcImage": "http://auction-image-clusterip:7778"
}